dspace ALL=(root) NOPASSWD: /bin/journalctl -u dspace
dspace ALL=(root) NOPASSWD: /bin/systemctl start dspace
dspace ALL=(root) NOPASSWD: /bin/systemctl stop dspace
dspace ALL=(root) NOPASSWD: /bin/systemctl status dspace
dspace ALL=(root) NOPASSWD: /usr/sbin/service dspace start
dspace ALL=(root) NOPASSWD: /usr/sbin/service dspace stop
dspace ALL=(root) NOPASSWD: /usr/sbin/service dspace status
dspace ALL=(root) NOPASSWD: /bin/journalctl -u handle
dspace ALL=(root) NOPASSWD: /bin/systemctl start handle
dspace ALL=(root) NOPASSWD: /bin/systemctl stop handle
dspace ALL=(root) NOPASSWD: /bin/systemctl status handle
dspace ALL=(root) NOPASSWD: /usr/sbin/service handle start
dspace ALL=(root) NOPASSWD: /usr/sbin/service handle stop
dspace ALL=(root) NOPASSWD: /usr/sbin/service handle status
