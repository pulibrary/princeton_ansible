# {{ ansible_managed }}
# will be overwritten via OS upgrades or ansible
[client]
host     = localhost
user     = debian-sys-maint
password = {{ galera_deb_db_password }}
socket   = /var/run/mysqld/mysqld.sock
[mysql_upgrade]
host     = localhost
user     = debian-sys-maint
password = {{ galera_deb_db_password }}
socket   = /var/run/mysqld/mysqld.sock
basedir  = /usr
