# Preferred Rocky Linux mirrors (Princeton) â€” additive, does not disable stock repos
[rocky-baseos-princeton]
name=Rocky Linux $releasever - BaseOS (Princeton)
baseurl=http://{{ preferred_mirror.princeton }}/pub/rocky/$releasever/BaseOS/$basearch/os/
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-rockyofficial
skip_if_unavailable=False
cost=10

